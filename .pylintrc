[MASTER]
load-plugins=pylint_fastapi

[MESSAGES CONTROL]
disable = missing-docstring,invalid-name,W0611, R0903
ignore = .venv/*, tests/*


[BASIC]
good-names = i,j,k,ex
variable-rgx = [a-z_][a-z0-9_]{1,30}$

[IMPORTS]
known-third-party = fastapi,sqlalchemy

[TYPECHECK]

generated-members=typing_extensions,pydantic
